<h2>Deals for Shop: {{shopName}}</h2>

<form action="/shop-deals/{{shopId}}" method="GET">
    <table>
        <thead>
            <tr>
                <th>Deal ID</th>
                <th>Quantity</th>
                <th>Price</th>
     
            </tr>
        </thead>
        <tbody>
            {{#each dealForShop}}
                <tr>
                    <td>{{this.qty}}</td>
                    <td>{{this.price}}</td>
                
                </tr>
            {{/each}}
        </tbody>
    </table>
</form>